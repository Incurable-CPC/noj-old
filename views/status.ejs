<% layout(layoutView) %>
<% if (typeof cont != 'undefined') { %>
<% include ./partial/cont-info.ejs%>
<% } %>

<div class="row">
	<div class="col-sm-1"></div>
	<div class="col-sm-10">
		<table class="table table-striped table-hover">
			<tr>
				<th width="10%">Run ID</th>
				<th width="10%">Problem</th>
				<th width="10%">User</th>
				<th width="15%">Result</th>
				<th width="7%">Time</th>
				<th width="10%">Memory</th>
				<th width="10%">Lang</th>
				<th width="10%">Length</th>
				<th width="18%">Submit Time</th>
			</tr>
			<%-partial('./partial/sol', solList) %>
		</table>
	</div>
	<div class="col-sm-1"></div>
	</div>
<div class="row">
	<div class="col-sm-2"></div>
	<div class="col-sm-8">
			<nav>
			<ul class="pager">
				<% if (page != 1) {%>
				<li class="previous">
					<a href="<%=url+String(page-1)%>">
						<span aria-hidden="true">&larr;</span> Prev
					</a>
				</li>
				<% } %>
				<li class="next">
					<a href="<%=url+String(page+1)%>">
						Next <span aria-hidden="true">&rarr;
						</span>
					</a>
				</li>
			</ul>
		</nav>
	</div>
	<div class="col-sm-2"></div>
</div>
