<% layout('layout') %>

<div class="contain">
	<div class="row">
		<div class="col-sm-3">
			<table class="table table-striped table-hover">
				<tr>
					<th>Submit</th>
					<td><%= pro.submit %></td>
				</tr>
				<tr>
					<th>User(Tried)</th>
					<td><%= pro.userTried %></td>
				</tr>
				<tr>
					<th>User(Solved)</th>
					<td><%= pro.userSolved %></td>
				</tr>
				<% for (var i = STATUS.AC; i <= STATUS.CE; i++) { %>
					<tr>
						<th><%=status[i] %></th>
						<td><%=pro.result[i] %></td>
					</tr>
				<% } %>
			</table>
		</div>
		<div class="col-sm-9">
			<table class="table table-striped table-hover" id="status">
				<tr>
					<th width="10%">Run ID</th>
					<th width="0">Problem</th>
					<th width="10%">User</th>
					<th width="10%">Result</th>
					<th width="10%">Time</th>
					<th width="10%">Memory</th>
					<th width="10%">Lang</th>
					<th width="15%">Code Length</th>
					<th width="25%">Submit Time</th>
				</tr>
				<%-partial('../partial/sol', pro.solList) %>
			</table>
		</div>
	</div>
</div>
